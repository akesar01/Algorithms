def calculate(b,c):
    a=list(zip(c,b))
    a.sort()
    print(a)
    count=1
    for i in range(1,len(a)):
        if (int(a[i-1][0])<=int(a[i][1])):
            count =count+1
    print(count)
        
    
        
    
    
    
    



a=int(input("enter the size of list"))
b=[]
c=[]
for i in range(a):
    b.append(input("Enter next number of starting array"))
    c.append(input("Enter next number of final time array"))
calculate(b,c)
