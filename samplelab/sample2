def minimum(a,b,x):
    minl={}
    for i in range(len(a)):
        for j in range(len(b)):
            m=a[i] +b[j]-x
            m=abs(m)
            minl[m]=[a[i],b[j]]
    e=min(minl.keys())
    for i in minl[e]:
        print(i,end=" ")
    





ar1=[1,4,5,7]
ar2=[10,20,30,40]
x=32
minimum(ar1,ar2,x)
